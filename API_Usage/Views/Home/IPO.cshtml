@model IEnumerable<API_Usage.Models.UpcomingIPO>

@{
    ViewData["Title"] = "IPO";
}

<section>
    <div class="container">
        <h2>Comming IPOs</h2>
        <hr />
        @if (Model.Count() > 0)
        {
            <table>
                <thead>
                    <tr>
                        <th>
                            Symbol
                        </th>
                        <th>
                            Company
                        </th>
                        <th>
                            Price
                        </th>
                        <th>
                            Shares
                        </th>
                        <th>
                            Amount
                        </th>
                        <th>
                            Float
                        </th>
                        <th>
                            Percent
                        </th>
                        <th>
                            Market
                        </th>
                        <th>
                            Expected
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.symbol)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.company)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.price)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.shares)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.amount)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.floatValue)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.percent)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.market)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.expected)
                            </td>

                        </tr>
                    }
                </tbody>
            </table>
        }
        else
        {
            <p>No comming IPOs available</p>
        }
    </div>
</section>